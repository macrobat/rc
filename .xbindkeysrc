# mute
"amixer sset Master toggle"
    m:0x0 + c:121
    XF86AudioMute 

# want to free ctrl <up>/<down> for gvim text bubbling, use alt there
"amixer set Master 1.5dB- unmute"
    m:0x0 + c:122
    XF86AudioLowerVolume 

"amixer set Master 2dB- unmute"
    m:0x4 + c:116
    Control + Down 

# vol up
"amixer set Master 1.5dB+ unmute"
    m:0x0 + c:123
    XF86AudioRaiseVolume 

"amixer set Master 2dB+ unmute"
    m:0x4 + c:111
    Control + Up 

# launch apps
"gmrun"
    m:0x4 + c:66
    Control + Escape 

